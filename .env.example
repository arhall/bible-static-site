# Cloudflare Account Configuration
# Copy this file to .env and fill in your actual values
# Do not commit .env to version control

# Your Cloudflare Account ID (found in Cloudflare dashboard)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id

# KV Namespace IDs for caching
CLOUDFLARE_KV_CACHE_ID=your-kv-namespace-id
CLOUDFLARE_KV_CACHE_PREVIEW_ID=your-preview-kv-namespace-id

# KV Namespace IDs for entities
CLOUDFLARE_KV_ENTITIES_ID=your-entities-kv-namespace-id
CLOUDFLARE_KV_ENTITIES_PREVIEW_ID=your-entities-preview-kv-namespace-id